{
  "name": "express-solid-boiler",
  "version": "1.0.0",
  "description": "Boilerplate for express app using solid principle",
  "main": "index.ts",
  "scripts": {
    "migrate:make": "knex --knexfile src/Database/knexfile.ts migrate:make",
    "migrate:latest": "knex --knexfile src/Database/knexfile.ts migrate:latest",
    "migrate:rollback": "knex --knexfile src/Database/knexfile.ts migrate:rollback",
    "start": "ts-node -r tsconfig-paths/register server.ts",
    "dev": "nodemon"
  },
  "repository": {
    "type": "git",
    "url": "github.com/Omoalfa/express-solid-boiler"
  },
  "author": "Omoalfa",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "knex": "^2.4.2",
    "morgan": "^1.10.0",
    "pg": "^8.11.1",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.0",
    "typedi": "^0.10.0",
    "typescript": "^5.1.6",
    "winston": "^3.9.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "bin": {
    "express-solid-boiler": "bin/index.js" 
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "nodemon": "^2.0.22"
  }
}
